        <script type="text/javascript">
            (function($) {
                // config params
                var width = '758',
                    height = '300',
                    client = 'ca-espn-q-schedule',
                    channel = 'nfl bottom',
                    lang = 'null';

                // builds iframe HTML
                function buildIframe(width, height, client, channel) {
                    var path = '/espn/sponsoredlinks/adsense/iframe';
                    if (window.location.host === 'espndeportes.espn.go.com') {
                        path = '/sponsoredlinksDeportes/adsense/iframe';
                    } else if (window.location.host === 'search.espn.go.com') {
                        // need a better solution here
                        path = '/modules/sponsoredlinks/adsense/iframe';
                    }
                    var iframe = '<iframe seamless scrolling="no" frameborder="0" height="' + height + '" width="' + width + '" src="' + path + '?width=' + width + '&height=' + height + '&client=' + escape(client) + '&channel=' + escape(channel) + '&lang=' + lang + '"></iframe>';
                    return iframe;
                }

                // insert the iframe
                if (espn.geo) {
                    espn.geo.getLocation().done(function(loc) {
                        if (loc.country.isoCode !== 'usa' || espn.core.mobileApple || espn.core.android) {
                            // outside US or not on desktop
                            client = 'ca-espn-nq';
                        }
                        $('#splinks-59561').replaceWith(buildIframe(width, height, client, channel));
                    }).fail(function() {
                        $('#splinks-59561').replaceWith(buildIframe(width, height, 'ca-espn-nq', channel));
                    });
                } else {
                    // can't locate them, assume they are outside US
                    $('#splinks-59561').replaceWith(buildIframe(width, height, 'ca-espn-nq', channel));
                }
            })(jQuery);
        </script>
